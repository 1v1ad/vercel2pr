generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @default(autoincrement())
  vkId      Int     @unique
  firstName String?
  lastName  String?
  avatar    String?
  balance   Int     @default(0)
}
